{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-console":0,
    "prettier/prettier": "error",
    "array-bracket-spacing" : true,
    "block-spacing": true,
    "semi": "always"
  }
}
